#include <stdio.h>
#include <math.h>
int main (){
int i, flag,num,count;
for (num=2; count<=100; num++)
{
    flag=0;
    for (i=2;i<sqrt(num);i++)
    {
    if (num%i==0)
    flag=1;
    break;
    }
    if (flag==1){
        }
        else{
            printf (" %d",num);
    
        count++;
        }
            
}
return 0;
}
